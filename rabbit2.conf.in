loopback_users = none

log.console = true
log.console.level = debug
log.file.level = debug

listeners.tcp.default = 5673
listeners.ssl.default = 5682

management.tcp.port = 15673
management.ssl.port = 15682

management.ssl.cacertfile = @@CURDIR@@/certs/@@HOSTNAME@@/ca_certificate.pem
management.ssl.certfile   = @@CURDIR@@/certs/@@HOSTNAME@@/server_@@HOSTNAME@@_certificate.pem
management.ssl.keyfile    = @@CURDIR@@/certs/@@HOSTNAME@@/server_@@HOSTNAME@@_key.pem

ssl_options.cacertfile    = @@CURDIR@@/certs/@@HOSTNAME@@/ca_certificate.pem
ssl_options.certfile      = @@CURDIR@@/certs/@@HOSTNAME@@/server_@@HOSTNAME@@_certificate.pem
ssl_options.keyfile       = @@CURDIR@@/certs/@@HOSTNAME@@/server_@@HOSTNAME@@_key.pem
